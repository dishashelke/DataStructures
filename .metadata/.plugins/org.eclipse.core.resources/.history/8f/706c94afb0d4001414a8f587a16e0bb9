import java.util.Scanner;
import java.util.*;


public class Solution {

	public static void main(String[] args) {
		Stack stack = new Stack();
	    int parent = -1;
	    int order[] = {1, 2, 3};
	    for (Integer each: order){
	        if (parent != -1 && parent > each)
	            System.out.println("False");
	        while (stack){
	            int top = stack.pop();
	            if(top < x)
	                parent = top;
	            }
	        stack.append(each);
	    }
	    System.out.println("True");
}
